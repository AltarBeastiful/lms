<?xml version="1.0" encoding="UTF-8" ?>
<messages xmlns:if="Wt.WTemplate.conditions">
<!--FORMS message blocks-->

<message id="Lms.PlayQueue.save-to-playlist">
	<i class="fa fa-upload"></i>
</message>

<message id="Lms.PlayQueue.load-from-playlist">
	<i class="fa fa-download"></i>
</message>

<message id="Lms.PlayQueue.clear">
	<i class="fa fa-times-circle"></i>
</message>

<message id="Lms.PlayQueue.template">
	<div class="page-header">
		<h2>${tr:Lms.PlayQueue.playqueue}</h2>
		<small>${nb-tracks}</small>
		${save-btn} ${load-btn} ${clear-btn}
	</div>
	${entries}
	${show-more}
</message>

<message id="Lms.PlayQueue.play">
	<i class="fa fa-play"></i>
</message>

<message id="Lms.PlayQueue.delete">
	<i class="fa fa-remove"></i>
</message>

<message id="Lms.PlayQueue.template.entry">
	${play-btn} ${del-btn}
	${name} ${<if-has-release>}${release-name}${</if-has-release>} ${<if-has-artist>}${artist-name}${</if-has-artist>}
</message>

</messages>
