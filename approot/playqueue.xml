<?xml version="1.0" encoding="UTF-8" ?>
<messages xmlns:if="Wt.WTemplate.conditions">
<!--FORMS message blocks-->

<message id="btn-playqueue-save-btn">
	<i class="fa fa-upload"></i>
</message>

<message id="btn-playqueue-load-btn">
	<i class="fa fa-download"></i>
</message>

<message id="btn-playqueue-clear-btn">
	<i class="fa fa-times-circle"></i>
</message>

<message id="template-playqueue">
	<div class="page-header">
		<h2>${tr:msg-playqueue}</h2>
		 ${save-btn} ${load-btn} ${clear-btn}
	</div>
	${entries}
	${show-more}
</message>

<message id="btn-playqueue-entry-play-btn">
	<i class="fa fa-play"></i>
</message>

<message id="btn-playqueue-entry-del-btn">
	<i class="fa fa-remove"></i>
</message>

<message id="template-playqueue-entry">
	${pos} ${name} ${<if-has-release>}${release-name}${</if-has-release>} ${<if-has-artist>}${artist-name}${</if-has-artist>} ${play-btn} ${del-btn}
</message>

</messages>
