<?xml version="1.0" encoding="UTF-8" ?>
<messages xmlns:if="Wt.WTemplate.conditions">
<!--FORMS message blocks-->

<message id="Lms.Explore.Release.play">
	<i class="fa fa-play"></i>
</message>

<message id="Lms.Explore.Release.add">
	<i class="fa fa-plus"></i>
</message>

<message id="Lms.Explore.Release.template">
	${cover}
	<div class="page-header">
		<h2>${name}</h2>
		${<if-has-artist>}<h4>${artist-name}</h4>${</if-has-artist>}
		<h4>${<if-has-year>}${year}${</if-has-year>}${<if-has-orig-year>} (${orig-year})${</if-has-orig-year>}</h4>
		${clusters}
		${play-btn}${add-btn}
	</div>
	${tracks}
</message>

<message id="Lms.Explore.Release.template.entry">
	${<if-has-disc-number>}${disc-number}-${</if-has-disc-number>}${<if-has-track-number>}${track-number}. ${</if-has-track-number>}
	${<if-has-artist>}${artist-name} - ${</if-has-artist>}
	${name}
	${play-btn}${add-btn}
</message>


</messages>
